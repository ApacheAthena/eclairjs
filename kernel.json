{
  "display_name": "Spark 1.5.1 (Eclair)",
  "language": "javascript",
  "argv": [
    "<path to spark-kernel distribution>/bin/spark-kernel",
    "--interpreter-plugin",
    "eclair:org.eclairjs.nashorn.JavascriptInterpreter",
    "--default-interpreter",
    "eclair",
    "--nosparkcontext",
    "--profile",
    "{connection_file}"
  ],
  "env": {
    "SPARK_OPTS": "--jars file:<path to nashorn jar>",
    "SPARK_HOME": "<path to spark 1.5.1 distribution>"
  },
  "codemirror_mode": "scala"
}
